
package pruebas.json;

import com.google.gson.Gson;

public class Main {

	/**
	 * @param args
	 */
	public static void main(final String[] args) {
		final Prueba clase = new Prueba();
		clase.setNombre("Fary");
		clase.setNúmero(7);
		clase.setPerro(true);
		final Gson pruebaGson = new Gson();
		final String json = pruebaGson.toJson(clase);
		Prueba clase2 = pruebaGson.fromJson(json, Prueba.class);
		System.out.println(clase2);

	}

}
