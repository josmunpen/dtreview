
package a.plus.json;

import java.util.ArrayList;
import java.util.List;

public class Prueba {

	/**
	 * @param args
	 */
	public static void main(final String[] args) {
		final List<Integer> l1 = new ArrayList<Integer>();
		l1.add(0);
		l1.add(1);
		l1.add(2);
		l1.add(3);
		l1.add(4);
		l1.add(5);
		l1.add(6);
		l1.add(7);

		System.out.println(l1.subList(0, l1.size() / 3));
	}

}
